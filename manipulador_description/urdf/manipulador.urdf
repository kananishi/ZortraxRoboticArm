      <!-- Inertia matrix -->
      <inertia
        ixx="4.2189241908e-01"
        ixy="5.9379537743e-04"
        ixz="6.7296383368e-03" 
        iyy="3.8385869267e-01"
        iyz="2.9011387812e-04" 
        izz="5.6774468564e-01" 
